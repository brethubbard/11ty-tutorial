---
layout: "layout.webc"
---

<div class="container-fluid">
    <div class="row justify-content-start">
        <div webc:for="(card, index) of cards" class="col-sm-12 col-md-6 col-lg-4">
            <bs-card :title="card.title" :subtitle="card.description" :button="card.button">
                <p>
                    <img class="img-fluid" :src="card.src">
                </p>
            </bs-card>
        </div>
    </div>
</div>